{
  "compilerOptions": {
    "strict": true,
    "target": "esnext",
    "downlevelIteration": true,
    "esModuleInterop": false,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    // "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": false,
    "types": ["react/experimental"],
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
